{
	"compilerOptions": {
		"target": "es5",
		"lib": ["es5", "es6", "dom"],
		"jsx": "react",
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"module": "commonjs",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"noImplicitAny": true,
		"noImplicitThis": true,
		"skipLibCheck": true
	}
}